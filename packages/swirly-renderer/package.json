{
  "name": "@swirly/renderer",
  "description": "Renderer for Swirly marble diagrams.",
  "version": "0.13.10",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/timdp/swirly.git",
    "directory": "packages/swirly-renderer"
  },
  "homepage": "https://swirly.dev",
  "author": {
    "name": "Tim De Pauw",
    "url": "https://tmdpw.eu"
  },
  "main": "dist/index.js",
  "browser": "src/index.ts",
