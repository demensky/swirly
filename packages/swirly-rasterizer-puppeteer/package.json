{
  "name": "swirly-rasterizer-puppeteer",
  "description": "Rasterizer for Swirly marble diagrams, using Puppeteer.",
  "version": "0.13.10",
  "license": "MIT",
  "repository": "https://github.com/timdp/swirly",
  "homepage": "https://swirly.dev",
  "author": {
    "name": "Tim De Pauw",
    "url": "https://tmdpw.eu"
  },
  "main": "dist/index.js",
  "files": [
