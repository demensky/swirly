{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts"],
      "outputs": ["dist/**"]
    },
    "@swirly/examples#build": {
      "dependsOn": ["^build"],
      "inputs": ["../../examples/*.txt"],
      "outputs": ["../../examples/*.svg", "../../examples/*.png"]
    },
    "clean": {
      "cache": false,
      "inputs": ["dist/**"],
      "outputs": []
    }
  }
}
